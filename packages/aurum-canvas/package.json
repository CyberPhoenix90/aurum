{
	"name": "aurum-canvas",
	"version": "0.0.21",
	"description": "Aurum style scene graph management for rendering in canvas",
	"main": "prebuilt/cjs/aurum-canvas.js",
	"typings": "prebuilt/cjs/aurum-canvas.d.ts",
	"scripts": {
		"build": "tsc && tsc -p tsconfig.amd.json && tsc -p tsconfig.cjs.json",
		"watch": "tsc -p tsconfig.cjs.json -w",
		"prepublish": "npm run build",
		"test": "tsc -p tsconfig.test.json && http-server -o -c-1"
	},
	"homepage": "https://aurumjs.org/",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/CyberPhoenix90/aurum-canvas"
	},
	"keywords": [
		"aurumjs",
		"scenegraph",
		"canvas",
		"rendering"
	],
	"author": "Joao Santos (CyberPhoenix)",
	"license": "MIT",
	"devDependencies": {
		"prettier": "^2.2.1",
		"typescript": "^4.3.2"
	},
	"dependencies": {
		"aurumjs": "0.6.22"
	}
}
