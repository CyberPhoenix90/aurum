{
    "name": "aurum-canvas",
    "version": "0.0.54",
    "description": "Aurum style scene graph management for rendering in canvas",
    "main": "prebuilt/esnext/aurum-canvas.js",
    "type": "module",
    "typings": "prebuilt/esnext/aurum-canvas.d.ts",
    "compiler": "typescript",
    "homepage": "https://aurumjs.org/",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/CyberPhoenix90/aurum"
    },
    "keywords": ["aurumjs", "scenegraph", "canvas", "rendering"],
    "author": "Joao Santos (CyberPhoenix)",
    "license": "MIT",
    "devDependencies": {
        "@types/chai": "^4.2.5",
        "@types/mocha": "^8.2.0",
        "@types/sinon": "^9.0.4",
        "chai": "^4.2.0",
        "mocha": "^8.2.1",
        "prettier": "^2.2.1",
        "typescript": "^5.6.2",
        "requirejs": "^2.3.6",
        "sinon": "^9.0.3"
    },
    "projectReferences": ["aurumjs"],
    "defaultBuildSettings": {
        "assembly": "library",
        "includes": ["src/**/*.ts", "src/**/*.tsx"],
        "jsxFragmentFactory": "Aurum.fragment",
        "jsxFactory": "Aurum.factory",
        "jsx": "react"
    },
    "builds": [
        {
            "outDir": "prebuilt/esnext",
            "name": "esnext",
            "platform": "isomorphic"
        }
    ]
}
