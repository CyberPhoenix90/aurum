{
    "name": "aurum-canvas",
    "version": "0.0.27",
    "description": "Aurum style scene graph management for rendering in canvas",
    "main": "prebuilt/cjs/aurum-canvas.js",
    "typings": "prebuilt/cjs/aurum-canvas.d.ts",
    "compiler": "typescript",
    "homepage": "https://aurumjs.org/",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/CyberPhoenix90/aurum-canvas"
    },
    "keywords": ["aurumjs", "scenegraph", "canvas", "rendering"],
    "author": "Joao Santos (CyberPhoenix)",
    "license": "MIT",
    "devDependencies": {
        "@types/chai": "^4.2.5",
        "@types/mocha": "^8.2.0",
        "@types/sinon": "^9.0.4",
        "chai": "^4.2.0",
        "mocha": "^8.2.1",
        "prettier": "^2.2.1",
        "typescript": "^4.3.2",
        "requirejs": "^2.3.6",
        "sinon": "^9.0.3"
    },
    "projectReferences": ["aurumjs"],
    "defaultBuildSettings": {
        "assembly": "library",
        "includes": ["src/**/*.ts", "src/**/*.tsx"],
        "jsxFragmentFactory": "Aurum.fragment",
        "jsxFactory": "Aurum.factory",
        "jsx": "react"
    },
    "builds": [
        {
            "outDir": "prebuilt/amd",
            "name": "amd",
            "platform": "browser"
        },
        {
            "outDir": "prebuilt/cjs",
            "name": "cjs",
            "platform": "node"
        },
        {
            "outDir": "prebuilt/exnext",
            "name": "esnext",
            "platform": "isomorphic"
        }
    ]
}
