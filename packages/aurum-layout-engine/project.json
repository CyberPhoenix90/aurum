{
    "name": "aurum-layout-engine",
    "version": "0.0.0",
    "main": "prebuilt/esnext/aurum-layout-engine.js",
    "typings": "prebuilt/esnext/aurum-layout-engine.d.ts",
    "compiler": "typescript",
    "homepage": "https://aurumjs.org/",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/CyberPhoenix90/aurum-layout-engine"
    },
    "author": "Joao Santos (CyberPhoenix)",
    "license": "MIT",
    "devDependencies": {
        "chai": "^4.2.0",
        "mocha": "^8.2.1",
        "prettier": "^2.2.1",
        "requirejs": "^2.3.6",
        "sinon": "^9.0.3"
    },
    "projectReferences": ["aurumjs"],
    "defaultBuildSettings": {
        "assembly": "library",
        "includes": ["src/**/*.ts", "src/**/*.tsx"],
        "jsxFragmentFactory": "Aurum.fragment",
        "jsxFactory": "Aurum.factory",
        "jsx": "react"
    },
    "builds": [
        {
            "outDir": "prebuilt/esnext",
            "name": "esnext",
            "platform": "isomorphic"
        }
    ]
}
